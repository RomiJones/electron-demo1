{
  "name": "demo1",
  "version": "0.0.1",
  "main": "main.js",
  "scripts": {
    "test": "standard",
    "start": "electron ./",
    "clean": "rm -rf SpaceRadar-darwin-x64",
    "build-win": "electron-packager . demo1 --platform=win32 --arch-ia32 --out ./OutApp --version 1.4.13 --overwrite --ignore=node_modules/hello/src --ignore=node_modules/hello/osx --ignore=node_modules/hello/binding.gyp --ignore=node_modules/hello/win32/.vs --ignore=node_modules/hello/win32/Debug --ignore=node_modules/hello/win32/Release --ignore=node_modules/hello/win32/binding* --ignore=node_modules/hello/win32/hello*  --ignore=node_modules/hello/win32/bin/Debug --ignore=node_modules/hello/win32/bin/Release/hello.pdb --ignore=node_modules/hello/win32/bin/Release/hello.exp --ignore=node_modules/hello/win32/bin/Release/hello.lib --ignore=node_modules/hello/win32/bin/Release/hello.map --icon=./logo_in_win.ico",
    "build-osx": "electron-packager . demo1 --platform=darwin --arch-x64 --out ./OutApp --version 1.4.13 --overwrite --ignore=node_modules/hello/src --ignore=node_modules/hello/win32 --ignore=node_modules/hello/binding.gyp --ignore=node_modules/hello/osx/binding*   --ignore=node_modules/hello/osx/bin/building.build --ignore=node_modules/hello/osx/bin/Debug --icon=./logo_in_osx.icns --download.mirror=https://npm.taobao.org/mirrors/electron/"
  },
  "dependencies": {
    "hello": "^1.0.0"
  }
}